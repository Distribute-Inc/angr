{
  "name": "angr",
  "version": "0.0.0",
  "description": "make angular2react components with ease (maybe not good ones though)",
  "license": "ISC",
  "main": "angr.js",
  "module": "angr.mjs",
  "types": [
    "src/index.d.ts"
  ],
  "files": [
    "angr.*"
  ],
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coveragePathIgnorePatterns": [],
    "mapCoverage": true,
    "moduleNameMapper": {},
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testMatch": [
      "**/*.spec.(ts|tsx|js)"
    ]
  },
  "reveal": true,
  "nyc": {
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "reporter": [
      "lcov",
      "json",
      "html",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules",
      "config",
      "coverage",
      "src/*.spec.js",
      "src/*.fixture.js",
      "angr.js",
      "angr.es.js"
    ],
    "include": [
      "src/*.js",
      "src/**/*.js",
      "!src/*.spec.js"
    ],
    "all": true,
    "check-coverage": true,
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "docs": "nps docs",
    "dependencies": "nps dependencies",
    "readme": "nps readme",
    "lint": "nps lint",
    "test": "nps test",
    "build": "nps build",
    "care": "nps care",
    "precommit": "nps precommit",
    "quickclean": "rm cool-hot-mess.tsx",
    "quicktest": "plop react2angular 'cool hot mess' test/fixture/component.html 'a: string, b: boolean, c?: any, d: boolean[]  '",
    "quick": "npm run quicktest && cat cool-hot-mess.tsx && npm run quickclean"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "css-to-json": "^2.1.0",
    "f-utility": "^3.4.1",
    "fluture": "^7.1.3",
    "globby": "^6.1.0",
    "htmltojsx": "^0.3.0",
    "inquirer-file-path": "^1.0.0",
    "katsu-curry": "^0.6.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/angular": "^1.6.31",
    "@types/commander": "^2.11.0",
    "@types/get-stdin": "^5.0.1",
    "@types/node": "^8.0.47",
    "angular": "1.6.6",
    "awesome-typescript-loader": "^3.2.3",
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-fbjs": "^2.1.4",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "cssjson-cli": "^1.0.1",
    "depcheck": "^0.6.7",
    "documentation": "^4.0.0-rc.1",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-eslint-comments": "^1.0.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "execa": "^0.8.0",
    "germs": "^0.1.9",
    "get-stdin": "^5.0.1",
    "husky": "^0.14.2",
    "inquirer-directory": "^2.1.0",
    "jayin": "^0.0.3",
    "jest": "^21.2.1",
    "jest-alias-preprocessor": "^1.1.1",
    "jest-cli": "^21.2.1",
    "jest-junit-reporter": "^1.1.0",
    "madge": "^2.0.0",
    "microtime": "^2.1.6",
    "nps": "^5.5.0",
    "nps-utils": "^1.2.0",
    "plop": "^1.9.0",
    "react2angular": "git://github.com/Distribute-Inc/react2angular.git#8cce1ca1a5231e43811fa711134eba3c0c0424a0",
    "rollup": "^0.49.2",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-babili": "^3.1.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-ts": "^0.1.0",
    "rollup-plugin-typescript2": "^0.5.2",
    "ts-jest": "^21.0.0",
    "tslib": "^1.7.1",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.5.3",
    "typescript-babel-jest": "^1.0.5",
    "typescript-eslint-parser": "^7.0.0",
    "wallaby-webpack": "^0.0.39",
    "webpack": "2.2.x",
    "xtrace": "^0.1.6"
  },
  "repository": "https://github.com/Distribute-Inc/angr",
  "author": "Distribute"
}

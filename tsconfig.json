{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "noLib": false,
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ],
    "types": [
      "node",
      "htmltojsx",
      "f-utility",
      "inquirer-file-path"
    ],
    "baseUrl": ".",
    "paths": {
    },
    "lib": [
      "dom",
      "es2016",
      "es2016.array.include",
      "es2017"
    ],
    "traceResolution": true
  },
  "awesomeTypescriptLoaderOptions": {
    "useBabel": true,
    "useWebpackText": true
  },
  "exclude": [
    "*.html",
    "*.scss",
    "*.spec.*",
    "*.fixture.*",
    "node_modules"
  ]
}
